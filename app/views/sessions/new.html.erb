<% provide(:title, "Log in") %>
<h1>投票（タイトル）</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(url: login_path, scope: :session) do |f| %>

      <%= f.label :email,"メールアドレス" %>
      <%= f.email_field :email, class: 'form-control' %>
<br>
      <%= f.label :password_digest,"パスワード" %>
      <%= f.password_field :password_digest, class: 'form-control' %>
<br>
      <%= f.submit "新規登録", class: "btn btn-primary" %>  
<br>
      <%= f.submit "決定", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
